{
	"name" : "creads/api2symfony-bundle",
	"description" : "A Symfony2 bundle to automatically generate controllers from standard API specifications (RAML, Blueprint, Swagger...)",
	"license" : "MIT",
	"type" : "symfony-bundle",
	"version" : "1.0.0",
	"keywords" : [
		"php",
		"raml",
		"symfony",
		"api",
		"converter",
		"bundle",
		"controller"
	],
	"homepage" : "http://creads.fr",
	"authors" : [{
			"name" : "Quentin",
			"email" : "q.pautrat@creads.org"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Creads\\Api2SymfonyBundle\\" : "src"
		}
	},
	"require" : {
		"symfony/http-kernel" : "3.0.1",
		"creads/api2symfony" : "dev-master",
		"symfony/filesystem" : "3.0.1",
		"symfony/twig-bundle" : "3.0.1"
	},
	"require-dev" : {
		"symfony/framework-bundle" : "3.0.1",
		"symfony/console" : "3.0.1",
		"symfony/yaml" : "3.0.1",
		"symfony/finder" : "3.0.1"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0.x-dev"
		}
	},
	"minimum-stability" : "dev",
	"repositories" : [{
			"type" : "vcs",
			"url" : "git@github.com:slavomirkuzma/php-raml-parser.git"
		}, {
			"type" : "vcs",
			"url" : "git@github.com:slavomirkuzma/api2symfony.git"
		}, {
			"type" : "vcs",
			"url" : "git@github.com:slavomirkuzma/api2symfony-bundle.git"
		}
	]
}